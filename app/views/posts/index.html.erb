<h1>Dribble table</h1>
<div class="pull-right"><%= link_to("新增貼文", new_post_path, class: "btn btn-info") %></div>
<% if !current_user %>
<div class="pull-right"><%= link_to("登入", new_user_session_path, class: "btn btn-primary") %></div>
<% else %>
<div class="pull-right"><%= link_to("登出", destroy_user_session_path,method: :delete, class: "btn btn-primary") %></div>
<% end %>
<% @posts.each do |post| %>
<p><%= link_to(post.title, post_path(post))  %></p>
<% end %>
